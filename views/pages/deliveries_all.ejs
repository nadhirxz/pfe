<!DOCTYPE html>
<%- include ('../partials/html') %>
<head>
	<%- include ('../partials/head') %>
</head>

<body>
    <%- include ('../partials/navbar') %>
    <div class="mx-3 text-center">
        <% if (locals.deliveries) { %>
			<h1 class="dlv mt-3 mb-5 font-weight-bold"></h1>
			<% if (Object.keys(deliveries).length) { %>
				<% locals.length = Object.keys(deliveries).length %>
				<% Object.keys(deliveries).forEach((date, index) => { %>
					<% locals.userDeliveries = deliveries[date]; locals.date = date; locals.index = index; %>
					<%- include ('../partials/user_deliveries') %>
				<% }); %>
			<% } else { %>
				<h4 class="mt-5 no_dlvrs"></h4>
			<% } %>
		<% } %>
    </div>
<%- include ('../partials/footer') %>
</body>
<%- include ('../partials/scripts') %>

</html>